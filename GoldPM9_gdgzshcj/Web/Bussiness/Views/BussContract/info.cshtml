            </td>
            <th width="15">合同名称</th>
            <td width="35%">
                <input name="ConName" style="width:98%;" class="easyui-textbox" validType="length[0,200]" data-options="required:true" value="@Model.ConName" />
            </td>

        </tr>

        <tr>
            <th width="15%">合同类别</th>
            <td width="35%">
                @BaseData.getBase(new Params()
           {
               controlName = "ConType",
               isRequired = true,
               engName = "ContractType",
               width = "98%",
               ids = Model.ConType.ToString()
           })
