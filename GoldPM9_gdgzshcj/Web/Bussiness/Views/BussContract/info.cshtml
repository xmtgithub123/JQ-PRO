                                $("#CustLinkManTel").textbox("setValue","");
                                $("#CustLinkManWeb").textbox("setValue","");
                                return true;
                            },
                            onLoadSuccess:function () {
                                $("#CustLinkId").combobox("setText",$("#CustLinkManName").val());
                            },
                        })

                    })
                </script>
