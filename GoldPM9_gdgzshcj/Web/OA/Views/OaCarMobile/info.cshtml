
@{
    Layout = "~/Areas/Core/Views/Mobile/_Layout.cshtml";
}
@model DataModel.Models.OaCarUse
@using JQ.Web;

@section head {
    @*<link rel="stylesheet" href="~/Scripts/jointjs/rappid.min.css" />*@

